{
  "name": "crunchycone-starter-template",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:open": "npm run dev & sleep 3 && (command -v xdg-open > /dev/null && xdg-open http://localhost:3000 || command -v open > /dev/null && open http://localhost:3000 || command -v start > /dev/null && start http://localhost:3000)",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "reset": "node scripts/reset.js",
    "setup-env": "node scripts/reset.js --jwt-only",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:reset": "rm -f db/prod.db && prisma migrate dev --name init && prisma db seed",
    "db:studio": "prisma studio",
    "prepare": "node scripts/install-hooks.js"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
